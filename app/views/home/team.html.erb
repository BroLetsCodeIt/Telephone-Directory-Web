  <!-- JavaScript for smooth scrolling -->
<script>
document.addEventListener('DOMContentLoaded', function() {
  // Function to handle smooth scrolling
  function scrollToSection(sectionId) {
    const section = document.getElementById(sectionId);
    if (section) {
      section.scrollIntoView({ behavior: 'smooth' });
    }
  }

  // Add click event listener to the "Web Team" button
  const webTeamButton = document.querySelector('a[href="#web-team"]');
  if (webTeamButton) {
    webTeamButton.addEventListener('click', function(event) {
      event.preventDefault(); // Prevent default anchor behavior
      scrollToSection('team'); // Scroll to the Web Team section
    });
  }

  // Add click event listener to the "App Team" button
  const appTeamButton = document.querySelector('a[href="#app-team"]');
  if (appTeamButton) {
    appTeamButton.addEventListener('click', function(event) {
      event.preventDefault(); // Prevent default anchor behavior
      scrollToSection('app-team'); // Scroll to the App Team section
    });
  }
});
</script>



